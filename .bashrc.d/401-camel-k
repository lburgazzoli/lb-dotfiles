
####################################################################
# Camel K
####################################################################

function ck-start-minikube {
    minikube start \
        --profile camel-k \
        --extra-config=apiserver.enable-admission-plugins="LimitRanger,NamespaceExists,NamespaceLifecycle,ResourceQuota,ServiceAccount,DefaultStorageClass,MutatingAdmissionWebhook"
}
