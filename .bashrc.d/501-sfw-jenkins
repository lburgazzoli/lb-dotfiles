function jenkins {
    local DOCKER_IMAGE="jenkinsci/blueocean"
    local DOCKER_NAME="jenkinsci"
    local DOCKER_USER=${LOGNAME}

    docker run \
        --rm \
        --detach \
        --user root \
        --name ${DOCKER_NAME} \
        --volume /opt/data/sfw/tools/jenkins/data:/var/jenkins_home:Z \
        --publish 8080:8080 \
        --publish 50000:50000 \
        ${DOCKER_IMAGE}
}